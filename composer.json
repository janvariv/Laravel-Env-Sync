{
  "name": "janvariv/laravel-env-sync",
  "description": "A package that makes sure that your .env file is in sync with your .env.example",
  "type": "library",
  "autoload": {
    "psr-4": {
      "Janvariv\\LaravelEnvSync\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Janvariv\\LaravelEnvSync\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": "^7.3",
    "vlucas/phpdotenv": "^5.0",
    "illuminate/console": "^8.5",
    "illuminate/support": "^8.5",
    "illuminate/events": "^8.5"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "mikey179/vfsstream": "^1.6",
    "mockery/mockery": "^1.0",
    "orchestra/testbench": "^5.0"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Viktor Janvari / Julien Tant",
      "email": "janvariv@gmail.com / julien@craftyx.fr"
    }
  ],
  "extra": {
    "laravel": {
      "providers": [
        "Janvariv\\LaravelEnvSync\\EnvSyncServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable"
}
